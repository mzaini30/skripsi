<script src="https://anzdoc.com/assets/js/jquery-ui.min.js"></script>
<link rel="stylesheet" href="https://anzdoc.com/assets/css/jquery-ui.css">
<script>
	$(function () {
	    $("#document_search").autocomplete({
		source: function (request, response) {
		    $.ajax({
			url: "https://anzdoc.com/suggest",
			dataType: "json",
			data: {
			    term: request.term
			},
			success: function (data) {
			    response(data);
			}
		    });
		},
		autoFill: true,
		select: function( event, ui ) {
		    $(this).val(ui.item.value);
		    $(this).parents("form").submit();
		}
	    });
	});
    </script>
